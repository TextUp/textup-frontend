{{#slideout-pane
    headerComponent=(component "slideout-pane/title"
        title=(service-prop "sharingSlideoutService" "slideoutTitle"))
    footerComponent=(component "slideout-pane/actions"
        showFooter=(service-prop "sharingSlideoutService" "anySharingChanges")
        onPrimary=(service-action "sharingSlideoutService" "finishSlideout")
        primaryLabel="Update"
        primaryProgressLabel="Updating...")
    direction=(constant "SLIDEOUT.DIRECTION.RIGHT")
    onClose=(service-action "sharingSlideoutService" "cancelSlideout") as |slideout|}}
    {{multi-toggle/share anyChanges=(get (service "sharingSlideoutService") "anySharingChanges")
        contacts=(service-prop "sharingSlideoutService" "contacts")
        staffs=(service-prop "sharingService" "staffCandidates")}}
{{/slideout-pane}}
