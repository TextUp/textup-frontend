{{#slideout-pane
    headerComponent=(component "slideout-pane/title" title="New Staff")
    footerComponent=(component "slideout-pane/actions"
        showFooter=true
        onPrimary=(service-action "slideout/staffService" "finishSlideout")
        disablePrimary=(service-prop "slideout/staffService" "shouldDisablePrimaryAction")
        primaryLabel="Create")
    onClose=(service-action "slideout/staffService" "cancelSlideout")
    forceKeepOpen=(service-prop "slideout/staffService" "shouldForceKeepOpen")
    focusSelector=".start-focus"}}
    {{model-form/staff staff=(service-prop "slideout/staffService" "staff")
        firstControlClass="start-focus"
        canModifyPhone=true
        onSearchAvailableNumbers=(service-action "numberService" "listAvailable")}}
{{/slideout-pane}}
