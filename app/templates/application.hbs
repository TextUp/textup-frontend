{{title "TextUp"}}

{{#liquid-if (get slideoutService.shouldShow constants.SLIDEOUT.OUTLET.DEFAULT)
	class='animate-slide-from-left animated-overflow'}}
	<div class="{{if isLocked 'hidden'}}">
		{{outlet constants.SLIDEOUT.OUTLET.DEFAULT}}
	</div>
{{/liquid-if}}

<div id="container">
	{{#lock-container
	  val= lockCode
	  doUpdateVal= (action (mut lockCode))
	  doValidate=(route-action 'verifyLockCode')
	  timeout= authService.authUser.org.content.timeout
	  username= authService.authUser.name
	}}
	  {{liquid-outlet}}
	{{/lock-container}}
</div>

{{notification-container notifications=notifications}}
{{loading-slider isLoading=loading}}
{{cookie-consent learnMoreLink=constants.LINK.PRIVACY_POLICY}}
{{cordova-script}}
