{{#multi-toggle tabindex=tabindex
	disabled=disabled
	wrapAround=wrapAround as |toggle doRegister|}}
    {{#multi-toggle/item doRegister=doRegister
        isSelected=initialSelectFalse
        color='#d3d3d3'
        onSelect=(action (mut value) false)}}
        {{falseString}}
    {{/multi-toggle/item}}
    {{#multi-toggle/item doRegister=doRegister
    	isSelected=(not initialSelectFalse)
        color='#76c9ec'
        onSelect=(action (mut value) true)}}
        {{trueString}}
    {{/multi-toggle/item}}
{{/multi-toggle}}
